
@{
    ViewBag.Title = "Index";
}

<h2>Upload File</h2>

<body>
    <div>
        @using (Html.BeginForm("Index", "Upload", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <br />
            <span>Select File:</span>
            <input type="file" name="postedFile" accept=".csv" />
            <hr />
            <input type="submit" value="Upload" />
            <br />
            <span style="color:green">@ViewBag.Message</span>
            <span style="color:red">@ViewBag.MessageError</span>
            <span style="color:red">@ViewBag.MessegeExisct</span>
        }
    </div>
</body>